{
  "script": "--exec babel-node -- ./server.js",
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "*.scss",
    "*.jsx",
    "./public/",
    "gulpfile.js"
  ],
  "verbose": true,
  "env": {
    "NODE_ENV": "development"
  },
  "watch": [
    ".",
    "server/"
  ],
  "ext": "js json"
}
